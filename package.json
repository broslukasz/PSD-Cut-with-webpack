{
  "name": "PSD cut with Webpack",
  "version": "1.0.0",
  "description": "",
  "main": "bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:home": "cross-env PAGE_NAME=home webpack-dev-server --port 7000 --progress --colors --watch --inline",
    "start:ilustrations": "cross-env PAGE_NAME=ilustrations webpack-dev-server --port 7000 --progress --colors --watch --inline",
    "build:all": "npm run build:home && npm run build:ilustrations",
    "build:home": "cross-env BUILD=true PAGE_NAME=home webpack --config",
    "build:ilustrations": "cross-env BUILD=true PAGE_NAME=ilustrations webpack --config",
    "build:minify-home": "cross-env BUILD=true PAGE_NAME=index webpack -p --config",
    "build:minify-ilustrations": "cross-env BUILD=true PAGE_NAME=ilustrations webpack -p --config"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "vanilla-modal": "^1.3.0"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "node-sass": "^3.10.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.15.0"
  }
}
